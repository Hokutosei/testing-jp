<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <title><%= "DB移行" %></title>
  <%= javascript_include_tag :defaults %>
  <script type="text/javascript">
    function input_loading(that){
       var td = that.up();
       td.update("<img src='/images/loding.gif' align='absmiddle'/>");
    }
  </script>
  <script src='/javascripts/async.js'></script>
  <script src='/javascripts/jp_async.js'></script>
