  <style
 type="text/css">
#navcontainer ul
{
  padding-left: 0;
  margin-left: 0;
  color: White;
  font-family: arial, helvetica, sans-serif;
}

#navcontainer ul li { display: inline; }

#navcontainer ul li a
{
  padding: 0.4em 1em;
  background-color: #6B8DE2;
  color: White;
  text-decoration: none;
  float: left;
  border-right: 1px solid #fff;
}

#navcontainer ul li a:hover, navcontainer ul li a.on
{
  background-color: #EC9337;
  color: #fff;
}
.inputW50 {
  width: 50px;
}

.inputW100 {
  width: 100px;
  font-size:14px;
}

.inputW250 {
  width: 150px;
}

p, div {
  margin: 0;
  padding: 0;
}

p.notice {
  line-height: 20px;
  text-align: center;
  background: #FFFDCA;
  padding: 5px 0;
}

.csv_form {
  width: 768px;
  margin: 0 auto;
  margin-top:10px ;
  border: 1px solid #CBCBCB;
}

.csv_form p.bg {
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #CBCBCB;
  background: url("/images/bg_csv_form.png") repeat;
}
#center{text-align: center;}

.csv_form h4 {
  font-size: 14px;
  font-weight: normal;
  margin: 10px 0 10px 10px;
  text-align: left;
  padding-left: 50px;
  line-height: 20px;
}

.csv_form table {
  border-collapse: collapse;
}

.csv_form table th,
.csv_form table td {
  padding: 5px;
  font-weight: normal;
  text-align: left;
}

.csv_form .csv_btn_box {
  margin: 10px 0 20px 0;
  text-align: center;
}

.csv_btn_box .csv_output {
  width: 136px;
  height: 28px;
  border: none;
  background: url("/images/btn_output.jpg");
  cursor: pointer;
}

.csv_btn_box .csv_input {
  width: 136px;
  height: 28px;
  border: none;
  background: url("/images/btn_input.jpg");
  cursor: pointer;
}
.navcontainer{
  width: 768px;
  margin: 0 auto;
  margin-top: 20px;
}
.left{
    text-align: left;
    padding-left:3px;
}
.title{
    margin: 0;
    padding-left: 5px;
    font-weight: bold;

}

.benfan_notice{
    padding-left: 5px;
    font-size: 12px;
    text-align: left;
    color: #FD0202;
}

.tform{
    border-collapse:collapse;
    padding-bottom: 5px;
}
.tform th{
    text-align:left;
    font-weight:bold;
    background:#f2f2f2 no-repeat 10px 15px;
    vertical-align:top;
}
.tform th, .tform td{
    padding:8px;
    border:1px solid #b2b2b2;
}
  </style>
</head>
<body>
this is the page


<% if params[:action] != "login" %>
<div>
  <div style="float: right;"><%= link_to "ログアウト", :controller => "/admin/admin", :action => "logout" %></div>
  <div style="clear: both;height: 1px;"></div>
  <div id="navcontainer" class="navcontainer" >
    <ul id="navlist">
      <li><%= link_to "開発環境", :controller => "/admin/csv_output", :action => "index" %></li>
      <li><%= link_to "検証環境", :controller => "/admin/csv_output", :action => "test_env" %></li>
      <li><%= link_to "本番環境", :controller => "/admin/csv_output", :action => "benfan" %></li>
    </ul>
    <div style="clear: both;"></div>
  </div>
</div>
<% end %>
<% if flash[:notice] %>
    <p class="notice"><%= flash[:notice] %></p>
<% end %>
<% flash[:notice] = nil %>
<div >
  <%= yield %>
</div> 
<div style="float:right;position:relative; top:-810px;left:-100px">
	Kameiko Task List
</div>

